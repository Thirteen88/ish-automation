{
  "environment": "development",
  "version": "1.0.0",
  "server": {
    "port": 3000,
    "host": "localhost",
    "timeout": 60000,
    "maxConnections": 100,
    "enableCors": true
  },
  "orchestrator": {
    "headless": true,
    "timeout": 60000,
    "maxConcurrent": 5,
    "retryAttempts": 3,
    "retryDelay": 1000,
    "enableCache": true,
    "cacheMaxSize": 100,
    "cacheTTL": 3600
  },
  "api": {
    "rateLimit": {
      "enabled": true,
      "maxRequests": 100,
      "windowMs": 60000
    },
    "timeout": 30000,
    "maxRetries": 3
  },
  "security": {
    "enableEncryption": true,
    "encryptionAlgorithm": "aes-256-gcm",
    "enableAuditLog": true,
    "secretRotationDays": 90,
    "allowedOrigins": ["http://localhost:3000", "http://localhost:3001"]
  },
  "logging": {
    "level": "info",
    "format": "json",
    "enableConsole": true,
    "enableFile": true,
    "filePath": "./logs/app.log",
    "maxFileSize": 10,
    "maxFiles": 5
  },
  "monitoring": {
    "enabled": true,
    "healthCheckInterval": 30000,
    "metricsInterval": 60000,
    "alertThresholds": {
      "cpuPercent": 80,
      "memoryPercent": 85,
      "errorRate": 0.05
    }
  },
  "featureFlags": {
    "ultraParallelExecution": {
      "enabled": true,
      "rolloutPercentage": 100,
      "enabledUsers": []
    },
    "apiPoweredMode": {
      "enabled": true,
      "rolloutPercentage": 100,
      "enabledUsers": []
    },
    "advancedErrorRecovery": {
      "enabled": true,
      "rolloutPercentage": 100,
      "enabledUsers": []
    },
    "contentFormatting": {
      "enabled": true,
      "rolloutPercentage": 100,
      "enabledUsers": []
    },
    "healthMonitoring": {
      "enabled": true,
      "rolloutPercentage": 100,
      "enabledUsers": []
    }
  }
}
