{
  "environment": "production",
  "version": "1.0.0",
  "server": {
    "port": 8080,
    "host": "0.0.0.0",
    "timeout": 120000,
    "maxConnections": 1000,
    "enableCors": false
  },
  "orchestrator": {
    "headless": true,
    "timeout": 120000,
    "maxConcurrent": 20,
    "retryAttempts": 5,
    "retryDelay": 2000,
    "enableCache": true,
    "cacheMaxSize": 500,
    "cacheTTL": 7200
  },
  "api": {
    "rateLimit": {
      "enabled": true,
      "maxRequests": 1000,
      "windowMs": 60000
    },
    "timeout": 60000,
    "maxRetries": 5
  },
  "security": {
    "enableEncryption": true,
    "encryptionAlgorithm": "aes-256-gcm",
    "enableAuditLog": true,
    "secretRotationDays": 30,
    "allowedOrigins": []
  },
  "logging": {
    "level": "warn",
    "format": "json",
    "enableConsole": false,
    "enableFile": true,
    "filePath": "/var/log/ish-automation/app.log",
    "maxFileSize": 50,
    "maxFiles": 30
  },
  "monitoring": {
    "enabled": true,
    "healthCheckInterval": 15000,
    "metricsInterval": 30000,
    "alertThresholds": {
      "cpuPercent": 70,
      "memoryPercent": 75,
      "errorRate": 0.01
    }
  },
  "featureFlags": {
    "ultraParallelExecution": {
      "enabled": true,
      "rolloutPercentage": 100,
      "enabledUsers": []
    },
    "apiPoweredMode": {
      "enabled": true,
      "rolloutPercentage": 100,
      "enabledUsers": []
    },
    "advancedErrorRecovery": {
      "enabled": true,
      "rolloutPercentage": 100,
      "enabledUsers": []
    },
    "contentFormatting": {
      "enabled": true,
      "rolloutPercentage": 100,
      "enabledUsers": []
    },
    "healthMonitoring": {
      "enabled": true,
      "rolloutPercentage": 100,
      "enabledUsers": []
    }
  }
}
